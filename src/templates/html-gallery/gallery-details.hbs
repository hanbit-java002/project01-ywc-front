{{set root=".."}}
{{set pageId="gallery-details"}}
{{> header title='Zip-Doc' cssName='gallery-details'}}
<div class="zd-contents-wrapper">
	<div class="zd-contents">
		<section class="galdetails-summary">
			<div class="summary-header">

			</div>
			<div class="summary-contents">
				<div class="summary-contents-show">
					<div class="show-viewer">
						<ul class="show-viewer-lists">
						</ul>
						<div class="show-viewer-arrow common-arrow arrow-left">&lt;</div>
						<div class="show-viewer-arrow common-arrow arrow-right">&gt;</div>
					</div>
				</div>
				<div class="summary-contents-galinfo">
					<div class="galbtns">
						<div class="galbtns-inform">
							<div><i class="fa fa-paperclip"></i> 찜하기</div>
							<div><i class="fa fa-share-alt"></i> 공유하기</div>
						</div>
						<div class="galbtns-estimate"><i class="fa fa-share-alt"></i> 견적신청</div>
					</div>
				</div>
			</div>
		</section>
		<section class="galdetails-form">
			<div class="galbtns form-galbtns">
				<div class="galbtns-inform">
					<div><i class="fa fa-paperclip"></i> 찜하기</div>
					<div><i class="fa fa-share-alt"></i> 공유하기</div>
				</div>
				<div class="galbtns-estimate"><i class="fa fa-share-alt"></i> 견적신청</div>
			</div>
		</section>
		<section class="galdetails-galpartners">
			<div class="galdetails-galpartners-info">
				<div class="galpartners-title">시공업체 및 시공사례</div>
				<div class="galpartners-box">
					<div class="galpartners-img-box">
					</div>
					<div class="galpartners-contents">
						<div class="galpartners-contents-name">
						</div>
						<ul class="galpartners-contents-lists">
							<li>
								<div class="galpartners-contents-subtitle">전문분야</div>
								<div class="galpartners-contents-desc specialty"></div>
							</li>
							<li>
								<div class="galpartners-contents-subtitle">시공지역</div>
								<div class="galpartners-contents-desc range"></div>
							</li>
							<li class="galpartners-contents-subtitle-box">
								<div class="galpartners-contents-subtitle">주요시공사례</div>
								<div  class="galpartners-contents-desc example">
									·경기도 남양주시 별내동 아이파크<br>
									·경기도 용인시 동백동 동보노빌리티아파트<br>
									·경기도 수내동 파크타운 삼익아파트<br>
									·서울시 강남구 삼성동 웨딩컨설팅 사무실
								</div>
							</li>
						</ul>
					</div>
				</div>
			</div>
			<div class="galpartners-more">더보기&gt</div>
			{{>slide}}
		</section>
	</div>
</div>
{{> footer jsName='gallery-details'}}
